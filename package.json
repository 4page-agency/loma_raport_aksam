{
	"name": "loma-raport-aksam",
	"version": "1.6.0",
	"description": "LOMA RAPORT",
	"main": "main.js",
	"dependencies": {
		"axios": "^1.7.0"
	},
	"scripts": {
		"start": "electron .",
		"dist": "electron-builder"
	},
	"build": {
		"appId": "com.loma.raport",
		"productName": "LOMA RAPORT AKSAM",
		"directories": {
			"output": "dist"
		},
		"files": [
			"**/*",
			"!node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
			"!**/node_modules/.bin",
			"!**/*.{o,hprof,orig,pyc,pyo,rbc}",
			"!**/test{,s}/**",
			"!**/tests{,s}/**"
		],
		"win": {
			"target": "nsis",
			"icon": "assets/IT_loma.ico"
		},
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true
		}
	},
	"devDependencies": {
		"electron": "^25.9.8",
		"electron-builder": "^23.6.0"
	}
}
